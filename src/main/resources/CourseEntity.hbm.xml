<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name = "org.justinhoang.entity.CourseEntity" table = "course"
           schema = "learnfasttest">
        <id name = "id">
            <column name = "id" sql-type = "bigint"/>
        </id>
        <property name = "title">
            <column name = "title" sql-type = "varchar(50)" length = "50"/>
        </property>
        <property name = "section">
            <column name = "section" sql-type = "varchar(50)" length = "50"/>
        </property>
        <property name = "format">
            <column name = "format" sql-type = "varchar(50)" length = "50"/>
        </property>
        <property name = "mentor">
            <column name = "mentor" sql-type = "varchar(70)" length = "70"/>
        </property>
        <property name = "sectionStatus">
            <column name = "section_status" sql-type = "varchar(50)"
                    length = "50"/>
        </property>
        <property name = "meets">
            <column name = "meets" sql-type = "varchar(50)" length = "50"/>
        </property>
        <property name = "dates">
            <column name = "dates" sql-type = "varchar(50)" length = "50"/>
        </property>
        <property name = "description">
            <column name = "description" sql-type = "varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>