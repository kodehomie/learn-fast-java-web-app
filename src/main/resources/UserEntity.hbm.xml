<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name = "org.justinhoang.entity.UserEntity" table = "user"
           schema = "learnfasttest">
        <id name = "id">
            <column name = "id" sql-type = "bigint"/>
        </id>
        <property name = "birthdate">
            <column name = "birthdate" sql-type = "date"/>
        </property>
        <property name = "firstName">
            <column name = "first_name" sql-type = "varchar(75)" length = "75"/>
        </property>
        <property name = "lastName">
            <column name = "last_name" sql-type = "varchar(70)" length = "70"/>
        </property>
        <property name = "email">
            <column name = "email" sql-type = "varchar(75)" length = "75"/>
        </property>
        <property name = "phone">
            <column name = "phone" sql-type = "varchar(25)" length = "25"/>
        </property>
        <property name = "username">
            <column name = "username" sql-type = "varchar(70)" length = "70"/>
        </property>
        <property name = "password">
            <column name = "password" sql-type = "varchar(255)"/>
        </property>
        <property name = "createTime">
            <column name = "create_time" sql-type = "datetime"/>
        </property>
        <property name = "updateTime">
            <column name = "update_time" sql-type = "datetime"/>
        </property>
    </class>
</hibernate-mapping>