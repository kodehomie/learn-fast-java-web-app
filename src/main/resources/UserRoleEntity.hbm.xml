<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name = "org.justinhoang.entity.UserRoleEntity" table = "user_role"
           schema = "learnfasttest">
        <id name = "id">
            <column name = "id" sql-type = "bigint"/>
        </id>
        <property name = "roleName">
            <column name = "role_name" sql-type = "varchar(70)" length = "70"/>
        </property>
        <property name = "userUsername">
            <column name = "user_username" sql-type = "bigint"/>
        </property>
        <property name = "userId">
            <column name = "user_id" sql-type = "bigint"/>
        </property>
    </class>
</hibernate-mapping>